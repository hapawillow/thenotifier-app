{
  "cli": {
    "version": ">= 16.20.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "developmentClient": false,
        "distribution": "internal",
        "env": {
          "__DEV__": "true"
        }
      }
    },
    "beta": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}