{
  "expo": {
    "name": "The Notifier",
    "slug": "thenotifier",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "thenotifier",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hapawillow.thenotifier",
      "appleTeamId": "CW4NQJKB99",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to display events and schedule notifications.",
        "NSAlarmKitUsageDescription": "This app needs access to schedule alarms so you can be notified at specific times."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hapawillow.thenotifier"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "vibrate": false,
          "sounds": [
            "./assets/sounds/notifyme.wav"
          ],
          "channels": [
            {
              "id": "notifyme",
              "name": "The Notifier notifications",
              "importance": "high",
              "sound": "notifyme.wav"
            }
          ],
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-sqlite",
      [
        "expo-calendar",
        {
          "calendarPermission": "This app needs access to your calendar to display events and schedule notifications."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "68632d89-5de5-4cc2-9a59-7021ce29fb6d"
      }
    }
  }
}