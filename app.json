{
  "expo": {
    "name": "The Notifier",
    "slug": "thenotifier",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "thenotifier",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hapawillow.thenotifier",
      "appleTeamId": "CW4NQJKB99",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to display events and schedule notifications.",
        "NSAlarmKitUsageDescription": "This app needs access to schedule alarms so you can be notified at specific times."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/adaptive-icon-foreground-512x512.png",
        "backgroundImage": "./assets/images/adaptive-icon-background-512x512.png",
        "monochromeImage": "./assets/images/adaptive-icon-monochrome-432x432.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hapawillow.thenotifier"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-1024x1024.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon-white-96x96.png",
          "color": "#8ddaff",
          "vibrate": false,
          "sounds": [
            "./assets/sounds/thenotifier.wav"
          ],
          "channels": [
            {
              "id": "thenotifier",
              "name": "The Notifier notifications",
              "importance": "high",
              "sound": "thenotifier.wav"
            }
          ],
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-sqlite",
      [
        "expo-calendar",
        {
          "calendarPermission": "This app needs access to your calendar to display events and schedule notifications."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "68632d89-5de5-4cc2-9a59-7021ce29fb6d"
      }
    }
  }
}